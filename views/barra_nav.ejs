<script>
  listar_empresa();
//guardar datos de la empresa//
   function guardar(){
    var id_em = document.getElementById('id_em').value;
    var n_empresa = document.getElementById('n_empresa').value;
    var direccion_em = document.getElementById('direccion_em').value;
    var telefono_em = document.getElementById('telefono_em').value;
  
   const datos= new URLSearchParams();

   datos.append('id_em',id_em);
   datos.append('n_empresa',n_empresa);
   datos.append('direccion_em',direccion_em);
   datos.append('telefono_em',telefono_em);
   
  
   fetch('/inicio/regist',{
     method: 'Post',
     body:datos
   })
   .then(res=>res.text())
   .then(data=>{
    console.log(id_em);
     alert(data);
     document.getElementById('n_empresa').value = '';
     document.getElementById('direccion_em').value = '';
     document.getElementById('telefono_em').value = '';
     


     window.location="http://localhost:4000/inicioBienvenida/Bienvenida";

   });
   
}

//listar datos de la empresa//

function listar_empresa(){
      fetch('/inicio/listar_empresa',{
        method:'get'

    })
    .then(resp=>resp.json())
    .then(data=>{
      var html='';
      for (var i in data)
          {
        html+=`<span id="empresa">  ${ data[i].n_empresa} </span>`
          
        }
        document.getElementById("h3").innerHTML= html;
        
        
        
      



    });
    }





</script>